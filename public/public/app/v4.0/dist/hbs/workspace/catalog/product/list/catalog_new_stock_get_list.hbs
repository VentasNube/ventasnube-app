<div class="col-md-12">

    <div class="cell padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
            {{#with ws_lang_data}} {{m_catalog_add_stock_cost}}:{{/with}}
        </div>
        <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
            <!-- Tipo de moneda icono -->
            <span>{{currency.value}}</span>
            <!-- Input para editar precio -->
            <input id="edit_price_cost_var_{{variant.id}}" class="form-control" type="number" placeholder="Precio Costo"
                value="{{variant.cost_price.value}}">
        </div>

        <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
            <!-- Boton de guardar cambios -->
            <div class="col-lg-6">
                <button type="button" onclick="edit_cost_price(this)" doc_id="{{_id}}" variant_id="{{variant.id}}"
                    class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-primary line pull-right "
                    aria-haspopup="true" aria-expanded="false">
                    <span class="material-icons">add_circle</span>
                </button>
            </div>
            <!-- Boton deEliminar -->
        </div>
    </div>
    <div class="cell padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class=" padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
            {{#userCtx 'catalog' 'admin'}}

            <div class="Switch" onclick="">
                <input id="{{variant.id}}_status_stock_control_checkbox" name="switch_variation" {{#ifEquals
                    variant.available_quantity.status true }} checked {{/ifEquals}} value="" type="checkbox"
                    doc_id="{{_id}}" variant_id="{{variant.id}}" input_id="status"
                    onclick="stock_status_edit_checkbox(this)" />
                <label for="{{variant.id}}_status_stock_control_checkbox" class="label-primary"></label>
            </div>

            {{/userCtx}}
            {{#with ws_lang_data}} {{m_catalog_edit_stock_active}}:{{/with}}
        </div>

        <div class="padding-0  col-xs-6 col-sm-6 col-md-6 col-lg-6">
            <div class="input-group">
                <input {{#ifEquals variant.available_quantity.status false }} disabled {{/ifEquals}}
                    id="edit_stock_var_{{variant.id}}" class="form-control" type="number" placeholder="Cantidad"
                    value="{{variant.available_quantity.value}}">
                <span class="input-group-addon">
                    <button id="edit_stock_var_btn_{{variant.id}}_{{_id}}" {{#ifEquals variant.available_quantity.status
                        false }} disabled {{/ifEquals}} onclick="edit_stock_var(this)" doc_id="{{_id}}"
                        variant_id="{{variant.id}}"
                        class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-primary line pull-right "
                        data-toggle="dropdown" aria-haspopup="true" type="button" aria-expanded="false">
                        <span class="material-icons">add_circle</span>
                    </button>
                </span>
            </div>
        </div>
    </div>

    <div class="padding-0 padding-b-20 margin-t-20 col-xs-12 col-sm-12 col-md-12 col-lg-12">

        {{#userCtx 'catalog' 'admin'}}
        <div class="Switch" onclick="">
            <input id="{{variant.id}}_stock_status_lote_edit_checkbox" name="switch_variation" {{#ifEquals  variant.available_quantity.control_status true }} checked {{/ifEquals}}
             value="" type="checkbox" doc_id="{{{_id}}}" variant_id="{{variant.id}}" input_id="status" onclick="stock_status_lote_edit_checkbox(this)" />
            <label for="{{variant.id}}_stock_status_lote_edit_checkbox" class="label-primary"></label>
        </div>
        {{/userCtx}}

        {{#with ws_lang_data}} {{m_catalog_edit_stock_lotes}}:{{/with}}
        {{#available_stock variant.stock_invetary}}{{/available_stock}}

    </div>

</div>

<dic id="stock_lot_main_{{variant.id}}_{{_id}}" {{#ifEquals variant.available_quantity.control_status false }} hidden {{/ifEquals}}  class="col-md-12">

    <div class="padding-20 col-xs-12 col-sm-12 col-md-12 col-lg-12">

        {{#each variant.stock_invetary}}
        <li class="list-group-item ripple ">
            {{#ifEquals type "in" }}
            <span class="material-icons text-green">
                check_circle
            </span>
            {{/ifEquals}}
            {{#ifEquals type "out" }}
            <span class="material-icons text-red">
                highlight_off
            </span>
            {{/ifEquals}}

            <span class="material-icons" data-toggle="tooltip" data-placement="bottom" title=""
                data-original-title="Creado:{{in_datetime}}">
                history
            </span>
            <span class="material-icons text-green" data-toggle="tooltip" data-placement="bottom" title=""
                data-original-title="Stock inicial">
                moving
            </span>
            {{in_stock}}
            <span class="material-icons text-red" data-toggle="tooltip" data-placement="bottom" title=""
                data-original-title="Vendidos">
                trending_down
            </span>
            {{out_stock}}

            <div class="pull-right">

                {{#ifEquals type "in" }}
                <span class="margin-r-10">
                    {{#with ws_lang_data}} {{{m_catalog_edit_cost}}} {{/with}}:
                    {{currency_value}}{{cost_price}}
                </span>
                <span class="pull-right"> {{#with ws_lang_data}}
                    {{{m_catalog_edit_enable}}} {{/with}}
                    <span class="badge"> {{real_stock}}</span>
                </span>
                {{/ifEquals}}

            </div>
        </li>
        {{/each}}

    </div>

    <div role="tabpanel" aria-labelledby="item-edit-variants" aria-expanded="true" class="bg-gray-light padding-10 margin-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
      
        <span class="padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
            Agregar Stock
        </span>
        <div class="padding-0 margin-0  col-xs-6 col-sm-6 col-md-6 col-lg-6">
            <div class="input-group">
                <span class="input-group-addon">
                    <span class="material-icons" data-toggle="tooltip" data-placement="bottom" title=""
                        data-original-title="Agregar precio">
                        price_change
                    </span>
                </span>
                <input id="new_cost_stock_var_{{variant.id}}_{{{_id}}}" class="col-sm-4 form-control" type="number"
                    placeholder="Precio de costo" value="">

            </div>
        </div>
        <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
            <div class="input-group">
                <input id="add_stock_var_{{variant.id}}_{{{_id}}}" class="form-control" type="number" placeholder="Cantidad" value="">

                <span class="input-group-addon">
                    <button onclick="add_stock_var(this)" id="btn_add_stock_var_{{variant.id}}_{{{_id}}}" doc_id="{{{_id}}}"
                        variant_id="{{variant.idd}}" input_id="stock_invetary"
                        class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-primary line pull-right "
                        data-toggle="dropdown" aria-haspopup="true" type="button" aria-expanded="false">
                        <span class="material-icons">add_circle</span>
                    </button>
                </span>
            </div>
        </div>
        
    </div>

</dic>