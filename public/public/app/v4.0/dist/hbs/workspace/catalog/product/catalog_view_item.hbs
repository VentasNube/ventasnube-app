
    <!-- Cart Nav module -->
    <div class="righ_nav_header">
        <!--span class="material-icons">receipt</span>Nueva -->
       
    </div>
    <div class="view-product">
        {{#with product_doc}}
                <div class="header" >
                            <button id="" type="button" class="round left_nav_close btn btn-default right_nav_close pull-right ">
                                <span class="material-icons">close</span>
                            </button>
                        <!-- #first trae el primer item del array -->
                      {{#first variations}}
                        <!-- #first trae el primer item del array -->
                            {{#first pictures}}
                                <div style="width: 100%; background-image: url('{{{max}}}');" class="parallax img_card" >
                                    <!--img class="card_img_url" src="{{{max}}}" alt="200x200"-->
                                </div>
                            {{/first }}
                      {{/first}}
                </div>
                    <h1>{{name}}</h1> 
                    <p class="cat">
                        {{#each tags }}
                        <span class="s-card-cat pull-left">#{{this}}</span>
                        {{/each}}
                    </p>
                    <br>
                     <h3>{{descriptions}}</h3> 

                        <div class="main_input">
                            <span class=" material-icons">monetization_on</span>
                            <input name="card_prduct_val" class="card_product_val form-control" type="number"
                                placeholder="valor" value="{{item.doc.price}}">
                        </div>



                        <div class="main_input">
                            <span class=" material-icons">local_offer</span>
                            <input name="card_prduct_discount" class="card_product_discount form-control" type="number"
                                placeholder="Descuento" value="0">
                        </div>


                        
                        <div class="main_input" >
                            <span class=" material-icons">superscript</span>
                            <input name="card_prduct_quantity" class="card_product_quantity form-control" type="number"
                                placeholder="Cantidad" value="1">
                        </div>
                           <div class="main_input" >
                            <span class=" material-icons">superscript</span>
                            <input name="card_prduct_quantity" class="card_product_quantity form-control" type="number"
                                placeholder="Cantidad" value="1">
                        </div>
                           <div class="main_input" >
                            <span class=" material-icons">superscript</span>
                            <input name="card_prduct_quantity" class="card_product_quantity form-control" type="number"
                                placeholder="Cantidad" value="1">
                        </div>

        {{/with}}
    </div>
