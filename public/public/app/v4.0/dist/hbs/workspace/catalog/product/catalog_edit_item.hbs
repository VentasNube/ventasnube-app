<!-- CATALOG EDIT PRODUCT TEMPLATE -->
<div class="view-product">
    
    <!--- HEADER NAV  -->
    <div class="margin-0 padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <button data-toggle="tooltip" data-placement="bottom" title="Subir foto" data-original-title="Subir foto"
            type="button"
            class="left_nav_close right_nav_close margin-10 round btn btn-transparent btn-default pull-left ">
            <span class="material-icons">
                arrow_back
            </span>
        </button>
        <h3 class=" margin-0 padding-t-20">{{#with ws_lang_data}} {{{m_catalog_edit_tittle}}} {{/with}}</h3>
    </div>
    {{#with product_doc}}
    <div class="s-card-activ-footer ">
        <div class="panel-group " role="tablist" aria-multiselectable="true">
            <div class="panel panel-default">
                <div class="panel-heading" role="tab" id="edit-item-description">
                    <h4 class="panel-title">
                        <a role="button" data-toggle="collapse" data-parent="#accordion"
                            href="#edit-item-description-body" aria-expanded="false" aria-controls="collapseOne"
                            class="collapsed">
                            <div class="row">
                                <div class="input-group">
                                    <span class="input-group-addon">
                                        <span class="material-icons">
                                            drive_file_rename_outline
                                        </span>
                                    </span>  
                                     <input doc_id="{{_id}}"  input_id="name"  onfocusout="cat_edit_product(this)"  value="{{name}}"  class="form-control" type="text" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="{{name}}">
                                    
                                 
                                </div>
                            </div>
                        </a>
                    </h4>
                </div>

                <div id="edit-item-description-body" class="panel-collapse collapse" role="tabpanel"
                    aria-labelledby="edit-item-description" aria-expanded="false" style="height: 0px;">
                    <div class="panel-body">
                        <div class="row">
                            <!--- TAGS MAIN -->
                            <div class="padding-0 margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="chips_main">
                                    <span class="icon material-icons" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="{{#with ../ws_lang_data}} {{{m_catalog_edit_tags}}} {{/with}}">
                                        tag
                                    </span>
                                    <div class="chips_items_main">
                                        {{#each tags }}
                                            <div class="chips_item s-card-cat pull-left" val_text=" {{this}}">
                                                <a doc_id="{{../_id}}" new_tag=" {{this}}"  input_id="tags" href="#" onclick="dell_tag(this)">
                                                    <span class="button material-icons text-s lh-n"> highlight_off</span>
                                                </a>
                                                <label class="chips_text">
                                                    {{this}}
                                                </label>
                                            </div>
                                        {{/each}}
                                    </div>
                                    <input doc_id="{{_id}}"  input_id="tags" class="chips_input form-control" name="keywords"
                                        type="text" onkeypress="add_new_tag_press(event,this)" style="">
                                </div>
                            </div>

                            <!--- Categoria Sub categoria  -->
                            <div class="padding-0 margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="material-icons" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="{{#with ../ws_lang_data}} {{{m_catalog_edit_category}}} {{/with}}">
                                                category
                                            </span>
                                        </span>

                                         <a doc_id="{{../_id}}" new_tag=" {{this}}"  input_id="tags" href="#" onclick="get_all_tag(this)"></a>
                                        <select doc_id="{{../_id}}" input_id="cat" href="#" onclick="get_all_tag(this)" class="form-control md" name="cat" id="cat">
                                            {{#each ../category_list/category_list }}
                                                <option value="{{id}}">{{value}}</option>
                                            {{/each}}
                                        </select>


                                        <span class="input-group-addon">
                                            <div class="btn-group dropright">
                                                <button id="" type="button"
                                                    class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <span class="material-icons">add_circle</span>
                                                </button>
                                                <ul class="dropdown-menu pull-right">
                                                    <li class="header">{{#with ../ws_lang_data}} {{{m_catalog_edit_category}}} {{/with}}</li>
                                                    <li>

                                                        <input doc_id="{{_id}}"  input_id="tags" class="chips_input form-control" name="keywords"
                                                     type="text" onkeypress="add_new_cat_press(event,this)" style="">
                           
                                                    </li>
                                                </ul>
                                            </div>
                                        </span>
                                    </div>
                                </div>
                                <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="material-icons">
                                                grid_view
                                            </span>
                                        </span>
                                        <select class="form-control md" name="cars" id="cars">
                                            {{#each ../category_list/category_list }}
                                                <option value="{{id}}">{{value}}</option>
                                            {{/each}}
                                        </select>
                                        <span class="input-group-addon">
                                            <div class="btn-group dropright">
                                                <button id="" type="button"
                                                    class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <span class="material-icons">add_circle</span>
                                                </button>
                                                <ul class="dropdown-menu pull-right">
                                                    <li class="header"> {{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                    <li>
                                                        <input id="" class="form-control" type="text"
                                                            placeholder="Escribe la categoria..." value="">
                                                    </li>
                                                    <li class="margin-t-10 text-center">
                                                        {{#with product_doc}}
                                                        <button onclick="catalog_save_edit_item(this)"
                                                            product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                            type="button" class="cobrar-btn btn btn-primary">
                                                            {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                        </button>
                                                        {{/with}}
                                                    </li>
                                                </ul>
                                            </div>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <!--- Marca y modelo  -->
                            <div class="padding-0 margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="material-icons">
                                                copyright
                                            </span>
                                        </span>
                                        <select class="form-control md" name="cars" id="cars">
                                            {{#each ../category_list/category_list }}
                                            <option value="{{id}}">{{value}}</option>
                                            {{/each}}
                                        </select>
                                        <span class="input-group-addon">
                                            <div class="btn-group dropright">
                                                <button id="" type="button"
                                                    class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <span class="material-icons">add_circle</span>
                                                </button>
                                                <ul class="dropdown-menu pull-right">
                                                    <li class="header">{{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                    <li>
                                                        <input id="" class="form-control" type="text"
                                                            placeholder="Escribe la categoria..." value="">
                                                    </li>
                                                    <li class="margin-t-10 text-center">
                                                        {{#with product_doc}}
                                                        <button onclick="catalog_save_edit_item(this)"
                                                            product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                            type="button" class="cobrar-btn btn btn-primary">
                                                            {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                        </button>
                                                        {{/with}}
                                                    </li>
                                                </ul>
                                            </div>
                                        </span>
                                    </div>
                                </div>
                                <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="material-icons">
                                                flip_to_front
                                            </span>
                                        </span>
                                        <select class="form-control md" name="cars" id="cars">
                                            {{#each ../category_list/category_list }}
                                            <option value="{{id}}">{{value}}</option>
                                            {{/each}}
                                        </select>
                                        <span class="input-group-addon">
                                            <div class="btn-group dropright">
                                                <button id="" type="button"
                                                    class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                    data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                    <span class="material-icons">add_circle</span>
                                                </button>
                                                <ul class="dropdown-menu pull-right">
                                                    <li class="header">{{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                    <li>
                                                        <input id="" class="form-control" type="text"
                                                            placeholder="Escribe la categoria..." value="">
                                                    </li>
                                                    <li class="margin-t-10 text-center">
                                                        {{#with product_doc}}
                                                        <button onclick="catalog_save_edit_item(this)"
                                                            product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                            type="button" class="cobrar-btn btn btn-primary">
                                                           {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                        </button>
                                                        {{/with}}
                                                    </li>
                                                </ul>
                                            </div>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--- NAME -->
    {{/with}}
    <h3> {{#with ws_lang_data}} {{{m_catalog_edit_variant}}} {{/with}}</h3>
    {{#each product_doc/variations}}
    <div class="s-card-activ-footer">
        <div class="panel-group " role="tablist" aria-multiselectable="true">
            <div class="panel panel-default margin-b-10">
                <div class="panel-heading" role="tab" id="variants">
                    <h4 class="panel-title">
                        <div class="Switch" onclick="">
                            <input
                            {{#ifEquals status.value active }} checked {{/ifEquals}}
                                id="switch_variation_{{id}}" name="switch_variation"
 {{#ifEquals                    status 'active' }} checked="" {{/ifEquals}} value="{{{status.value}}}" name="switch_variation"
                                type="checkbox" doc_id="{{{../product_doc/_id}}}" variant_id="{{id}}"  input_id="status" onclick="cat_edit_product(this)"
                                />
                            <label for="switch_variation_{{id}}" class="label-primary"></label>
                        </div>
                        <a role="button" data-toggle="collapse" data-parent="#item-edit-variants"
                            href="#edit_variations_{{id}}" aria-expanded="false" aria-controls="collapseOne"
                            class="collapsed">
                            <div class="text-left btn-group">
                                <div class="s-card-mini-img ">
                                    {{#first pictures}}
                                    <img src="{{min}}" alt="">
                                    {{/first}}
                                </div>
                                <span>
                                    {{#each attribute_combinations as | ac |}}
                                    <!-- Hago la comprobacion de si es un color -->
                                    {{#ifEquals id 'SIZE' }}
                                    <div class="margin-t-10 round-icon pr" data-toggle="tooltip" data-placement="bottom"
                                        title="{{{id_name}}}" data-original-title="{{{id_name}}}">
                                        <span> {{value}} </span>
                                    </div>
                                    {{/ifEquals}}
                                    {{#ifDiff id 'COLOR' }}
                                    {{name}}
                                    {{/ifDiff}}
                                    {{/each}}

                                </span>
                            </div>
                            <div class="pull-right padding-10">
                                {{#with ws_lang_data}} Stock: {{/with}}
                                {{#stock stock_invetary}}{{/stock}}
                                <div class="round-icon pr"><span> {{../product_doc/currency.value}} </span></div>
                                {{#first price_list}}
                                <span iid="total_neto_prod"> {{value}}</span>
                                {{/first}}
                            </div>
                        </a>
                    </h4>
                </div>
                <!--- BODY VARIANTE --->
                <div id="edit_variations_{{id}}" class="panel-collapse collapse" role="tabpanel"
                    aria-labelledby="item-edit-variants" aria-expanded="false" style="height: 0px;">
                    <div class="panel-body">
                        <div class="row">
                              <!-- FOTOS -->
                            <div class="header">
                                <button id="" type="button"
                                    class="round margin-10 left_nav_close btn btn-default right_nav_close pull-right ">
                                    <span class="material-icons">
                                        delete_forever
                                    </span>
                                </button>
                                <button data-toggle="tooltip" data-placement="bottom" title="{{#with ../ws_lang_data}} {{{m_catalog_edit_add_pic}}} {{/with}}"
                                    data-original-title="{{#with ../ws_lang_data}} {{{m_catalog_edit_add_pic}}} {{/with}}" type="button"
                                    class="margin-10 round btn btn-transparent btn-default pull-left ">
                                    <span class="material-icons">
                                        add_a_photo
                                    </span>
                                </button>
                                {{#first pictures}}
                                    <div style="width: 100%; background-image: url('{{{max}}}');" class="parallax img_card">
                                        <!--img class="card_img_url" src="{{{max}}}" alt="200x200"-->
                                        <div class=" mini-carrousel-img ">
                                            {{#each ../pictures }}
                                            <div class="mini-card-img-s">
                                                <div class="img">
                                                    <a target="_blank" href="{{min}}">
                                                        <img src="{{min}}" alt="">
                                                    </a>
                                                </div>
                                            </div>
                                            {{/each}}
                                        </div>
                                    </div>
                                {{/first }}
                            </div>
                            <!-- DETALLE  COLOR ETC TALLE -->
                            <div class="padding-0 margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                <!--- DETALLE -->
                                <div class="padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <div class="input-group">
                                        <span class="input-group-addon">
                                            <span class="material-icons">
                                                description
                                            </span>
                                        </span>
                                        <input  doc_id="{{#with product_doc}}{{{../../product_doc/_id}}}{{/with}}" variant_id="{{id}}"  input_id="description"  onfocusout="cat_edit_product(this)"  class="form-control" type="textarea" placeholder="{{#with ../ws_lang_data}} {{{m_catalog_edit_description}}} {{/with}}"
                                            value="{{description.value}}">
                                        <!--- ATRIBUTOS DE LA VARIABLE -->
                                    </div>
                                </div>
                                <!---COLOR-->

                                {{#with color }}
                                    <div class="padding-0 col-lg-6 col-md-6 col-sm-6 col-xs-6">
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <input id="{{{../../product_doc/_id}}}_{{../id}}_color_status" type="checkbox" {{#ifEquals status false }} checked {{/ifEquals}} doc_id="{{{../../product_doc/_id}}}" variant_id="{{../id}}"  input_id="color" input_status="{{{status}}}" value="{{{value}}}" onclick="cat_edit_product(this)" aria-label="...">
                                                    </span>
                                                    <label for="" >
                                                        <input status="{{{status}}}" id="{{{../../product_doc/_id}}}_{{../id}}_color" value="{{{value}}}" {{#ifEquals status true }} disabled="" {{/ifEquals}} doc_id="{{{../../product_doc/_id}}}" variant_id="{{../id}}"  input_id="color" onfocusout="cat_edit_product(this)"  class="form-control md padding-10" type="color" placeholder="{{#with ../ws_lang_data}} {{{m_catalog_edit_color}}} {{/with}}" >
                                            
                                                    </label>
                                                </div>
                                </div>
                                {{/with}} 
                                {{#with size }}
                                <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                                                            <div class="input-group">
                                                                                <span class="input-group-addon">
                                                                                    <input id="{{{../../product_doc/_id}}}_{{../id}}_size_status" type="checkbox" {{#ifEquals status false }} checked {{/ifEquals}} doc_id="{{{../../product_doc/_id}}}" variant_id="{{../id}}"  input_id="size" input_status="{{{status}}}" value="{{{value}}}" onclick="cat_edit_product(this)" aria-label="...">
                                                                                </span>
                                                                                <label for="" >
                                                                                    <select onchange="cat_edit_product(this)" class="form-control md" name="size" status="{{{status}}}" id="{{{../../product_doc/_id}}}_{{../id}}_size" value="{{{value}}}" {{#ifEquals status true }} disabled="" {{/ifEquals}} doc_id="{{{../../product_doc/_id}}}" variant_id="{{../id}}"  input_id="size"  >
                                                                                                <option selected value="{{{value}}}">{{{value}}}</option>
                                                                                            {{#each ../../attributes_list/size }}
                                                                                                <option value="{{value}}">{{value}}</option>
                                                                                            {{/each}}
                                                                                    </select>
                                                                                </label>
                                                                                <span class="input-group-addon">
                                                                                        <div class="btn-group dropright">
                                                                                            <button id="" type="button"
                                                                                                class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                                                                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                                                                <span class="material-icons">add_circle</span>
                                                                                            </button>
                                                                                            <ul class="dropdown-menu pull-right">
                                                                                                <li class="header">{{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                                                                <li>
                                                                                                    <input id="" class="form-control" type="text"
                                                                                                        placeholder="Escribe la categoria..." value="">
                                                                                                </li>
                                                                                                <li class="margin-t-10 text-center">
                                                                                                    {{#with product_doc}}
                                                                                                    <button onclick="catalog_save_edit_item(this)"
                                                                                                        product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                                                                        type="button" class="cobrar-btn btn btn-primary">
                                                                                                        {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                                                                    </button>
                                                                                                    {{/with}}
                                                                                                </li>
                                                                                            </ul>
                                                                                        </div>
                                                                                </span>
                                                                            </div>
                                </div>
                                {{/with}}                                           
                                {{#each attribute_combinations as | ac |}}

                                        <!--- weight --->
                                        {{#ifEquals id 'WEIGHT' }}
                                            <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <input type="checkbox" aria-label="...">
                                                    </span>

                                                    <select class="form-control md" name="cars" id="cars">
                                                        <option value="">{{#with ../ws_lang_data}} {{{m_catalog_edit_peso}}} {{/with}}</option>
                                                    </select>
                                                    <span class="input-group-addon">
                                                        <div class="btn-group dropright">
                                                            <button id="" type="button"
                                                                class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                                <span class="material-icons">add_circle</span>
                                                            </button>
                                                            <ul class="dropdown-menu pull-right">
                                                                <li class="header" {{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                                <li>
                                                                    <input id="" class="form-control" type="text"
                                                                        placeholder="Escribe la categoria..." value="">
                                                                </li>
                                                                <li class="margin-t-10 text-center">
                                                                    {{#with product_doc}}
                                                                    <button onclick="catalog_save_edit_item(this)"
                                                                        product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                                        type="button" class="cobrar-btn btn btn-primary">
                                                                    {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                                    </button>
                                                                    {{/with}}
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </span>
                                                </div><!-- /input-group -->
                                            </div>
                                        {{/ifEquals}}  

                                         <!--- PACK --->
                                        {{#ifEquals id 'PACK' }}
                                            <div class="padding-0 col-xs-6 col-sm-6 col-md-6 col-lg-6">
                                                <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <input type="checkbox" aria-label="...">
                                                    </span>
                                                    <select class="form-control md" name="cars" id="cars">
                                                        <option value="">Pack x1</option>
                                                        <option value="">Pack x2</option>
                                                            <option value="">Pack x6</option>
                                                            <option value="">Pack x12</option>
                                                    </select>
                                                    <span class="input-group-addon">
                                                        <div class="btn-group dropright">
                                                            <button id="" type="button"
                                                                class="round padding-0  margin-5 dropdown-toggle dropdown-toggle-split btn btn-default pull-right "
                                                                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                                <span class="material-icons">add_circle</span>
                                                            </button>
                                                            <ul class="dropdown-menu pull-right">
                                                                <li class="header"> {{#with ../ws_lang_data}} {{{m_catalog_edit_new}}} {{/with}}</li>
                                                                <li>
                                                                    <input id="" class="form-control" type="text"
                                                                        placeholder="Escribe la categoria..." value="">
                                                                </li>
                                                                <li class="margin-t-10 text-center">
                                                                    {{#with product_doc}}
                                                                    <button onclick="catalog_save_edit_item(this)"
                                                                        product_id="{{{../../product_doc/_id}}}" variant_id="{{id}}"
                                                                        type="button" class="cobrar-btn btn btn-primary">
                                                                        {{#with ../ws_lang_data}} {{{m_catalog_edit_add}}} {{/with}}
                                                                    </button>
                                                                    {{/with}}
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </span>
                                                </div><!-- /input-group -->
                                            </div>
                                        {{/ifEquals}}

                                {{/each}}
                                <!--- SKU --->
                                <div class="input-group padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <span class="input-group-addon">
                                        <span class="material-icons">
                                            qr_code
                                        </span>
                                    </span>
                                    <input doc_id="{{#with product_doc}}{{{../../product_doc/_id}}}{{/with}}" variant_id="{{id}}"  input_id="sku"  onfocusout="cat_edit_product(this)"  class="form-control" type="text" placeholder="Codigo universal SKU"
                                        value="{{sku.value}}">
                                </div>
                            </div>
                            <!-- PRECIOS INVENTARIO -->
                            <div class="tabs-main margin-t-10 default col-xs-12 col-sm-12 col-md-12 col-lg-12 padding-0">
                                <ul class="nav nav-tabs">
                                    <li id="" class="ripple  active">
                                        <a data-toggle="tab" href="#edit_price_item_tab_{{id}}">
                                            <span class="material-icons">
                                                price_change
                                            </span>
                                            {{#with ../ws_lang_data}} {{{m_catalog_edit_price}}} {{/with}}
                                        </a>
                                    </li>
                                    <li id="" class="ripple">
                                        <a data-toggle="tab" href="#edit_stock_item_tab_{{id}}">
                                            <span class="material-icons">
                                                production_quantity_limits
                                            </span>
                                            <span class="">{{#stock stock_invetary}}{{/stock}}</span>
                                             {{#with ../ws_lang_data}} {{{m_catalog_edit_stock}}} {{/with}}
                                        </a>
                                    </li>
                                </ul>
                                <!--tab-content--->
                                <div class="tab-content ">
                                    <!--tab--->
                                    <div id="edit_price_item_tab_{{id}}" class="tab-pane fade in active">
                                        <!--s-card-activ-cont--->
                                        <ul class="list-group margin-0">
                                            {{#each price_list as | pl |}}
