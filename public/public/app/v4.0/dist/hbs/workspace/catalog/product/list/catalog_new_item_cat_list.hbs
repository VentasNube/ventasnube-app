{{#each cat_find_list }}
<li onclick='catalog_select_new_cat(this)' item_value_id="{{id}}" item_value="{{value}}" item_id="category">
    <div class="dropdown-item">
         <a  new_value="{{value}}" onclick='catalog_product_edit_category(this)' 
                 input_value="{{category.id}}" 
                  input_id="category" href="#"  
                   doc_id="{{../_id }}" 
                    value="{{value}}">
               {{value}}
          </a>
        {{#userCtx 'catalog' 'admin'}}
        <button><span doc_id="{{../_id }}" value="{{value}}" class="material-icons"
                onclick='catalog_dell_cat(this)'>delete</span></button>

                 <button>
                      <span doc_id="" value="{{value}}"
                        class="material-icons"
                        onclick='catalog_dell_cat(this)'>delete</span>
                   </button>
        {{/userCtx}}
    </div>
</li>
{{/each}}
{{#if no_result }}
<div class="padding-30 margin-t-30 text-center">
    <button class="btn btn-default" onclick='catalog_add_new_cat(this)' data-toggle="tooltip"
        data-original-title="{{#with ws_lang_data}} {{{m_catalog_edit_add}}} {{{m_catalog_edit_new}}} {{{m_catalog_edit_variant}}} {{/with}} "
        data-placement="bottom">
        <span class="material-icons" title="">add_circle_outline</span>
        {{#with ws_lang_data}} {{{m_catalog_edit_add}}} {{{m_catalog_edit_new}}} {{{m_catalog_edit_category}}} {{/with}}
    </button>
</div>
{{/if}}