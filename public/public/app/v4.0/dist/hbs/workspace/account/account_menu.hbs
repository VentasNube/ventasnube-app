<!-- CATALOG CREATE NEW PRODUCT TEMPLATE -->
<div class="">

    <!--- HEADER NAV  -->
    <div class="margin-0 padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <button type="button"
            class="left_nav_close right_nav_close margin-10 round btn btn-transparent btn-default pull-left ">
            <span class="material-icons">
                arrow_back
            </span>
        </button>
        <h3 class=" margin-0 padding-t-20"> </h3>
    </div>

    {{#with user}}

    <div class="user-body-workspace padding-b-5">
        <div class="text-center user-body-workspace-title padding-b-10 {{{../ws_info.workspace_color}}}">
            <div class="round-icon user-top"> {{user_name}} </div>
            <p>{{user_name}}</p>
            <p>{{user_email}}</p>
            <p>
            <div class="text-center margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12"></div>
            <a href="/workspace/home" class="btn btn-default">
                <span class="material-icons">
                    {{#with ../ws_lang_data}} {{ws_home_icon}}{{/with}}
                </span>
                {{#with ../ws_lang_data}} {{m_my_workspace}}{{/with}}
            </a>
            <button onclick="get_ws_setting(this)" class="btn btn-default">
                <span class="material-icons">
                    {{#with ../ws_lang_data}} {{ws_config_icon}}{{/with}}
                </span>
                {{#with ../ws_lang_data}} {{m_config_workspace}}{{/with}}
            </button>
        </div>

        </p>
    </div>
    <div class="text-center margin-t-10 col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <!--button type="button" class="btn btn-primary"><span class="material-icons"> print </span> Imprimir </button-->
        <button onclick="account()" type="button" class="cobrar-btn btn btn-default">
            <span class="material-icons">
                {{#with ../ws_lang_data}} {{m_account_tittle_icon}}{{/with}}
            </span>

            {{#with ../ws_lang_data}} {{b_myaccount}}{{/with}}

        </button>
        <button onclick="logout()" type="button" class="btn btn-default">
            <span class="material-icons">
                {{#with ../ws_lang_data}} {{b_exit_icon}}{{/with}}
            </span>
            {{#with ../ws_lang_data}} {{b_exit}}{{/with}}
        </button>
    </div>

    {{/with}}

</div>