<!--- CATALOG BARRA NAVEGACION TOP TEMPLATE ( nav_bar.hbs )  --->
<div class="box">
    <div class="box-header">
        <div class="nav-header nav_bar_catalog_header">
        <div class="col-xs-2 col-sm-2 col-md-2 col-lg-2">

                <div class="btn-group dopdown">
                    <button id="" m_id="{{{m_id}}}" m_t_id="{{{m_t_id}}}" m_t_name="{{{m_t_name}}}" type="button"
                        class="dropdown-toggle  select-btn btn xl btn-more  dropdown-toggle btn-primary text"
                        data-toggle="dropdown" aria-expanded="true">
                        <span class="material-icons">
                            dashboard</span>
                        <span class="this_m_t_name">Ventas</span>
                        <i class="material-icons">expand_more</i>
                    </button>

                    <ul class="dropdown-menu pull-right">
                        {{#each ws_left_nav_data.m_t}}

                        <li class="margin-5">
                            <a m_id="{{{m_id}}}" m_t_id="{{{m_t_id}}}" s_url_t_m="{{{m_url}}}"
                                class="l_nav_t_m btn-status btn {{{m_t_color}}} text" href="#" role="button"
                                data-toggle="tooltip" data-placement="top" data-original-title="{{p_description}}">
                                <span class="material-icons">{{m_t_icon}}</span>
                                <span class="this_m_t_name">{{m_t_name}}</span>
                            </a>
                        </li>

                        {{/each}}
                    </ul>
                </div>
        </div>
            <div class="padding-0 col-xs-7 col-sm-7 col-md-7 col-lg-7">
                <div class="nav_bar_catalog_search">
                    <div class="input-filters">
                        <div class="group">
                            <div class="btn-group dopdown">
                                <button type="button" class="btn xl btn-more  dropdown-toggle btn-primary text"
                                    data-toggle="dropdown" aria-expanded="true">
                                    {{#with ws_lang_data}} {{m_board_filter}}{{/with}}
                                    <span class="material-icons"> filter_list </span>
                                </button>
                                <ul class="dropdown-menu xl pull-left">
                                    <!-- Con pull-right mueve para la derecha o hizquierda el poper-->
                                    <div class="body">
                                        <li class="header"> <span> {{#with ws_lang_data}} {{b_myaccount}}{{/with}}</span> Filtrar ordenes</li>
                                        <li class="divider"></li>
                                        <div class="padding-10 col-xs-12 col-sm-6 col-md-6">
                                            <li>
                                                <label>Cliente {{#each ws_lang_data}}
                                                    {{b_myaccount}}{{/each}}</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">account_circle</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input"
                                                        placeholder="Nombre del cliente" value="">
                                                </div>
                                            </li>
                                            <li>
                                                <label>Colaborador</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">group</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input"
                                                        placeholder="Nombre del cliente" value="">
                                                </div>
                                            </li>
                                        </div>
                                        <div class="padding-10  col-xs-12 col-sm-6 col-md-6">
                                            <li>
                                                <label>Prioridad</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">low_priority</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input"
                                                        placeholder="Nombre del cliente" value="">
                                                </div>
                                            </li>
                                            <li>
                                                <label>Estado</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">mood</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input"
                                                        placeholder="Nombre del cliente" value="">
                                                </div>
                                            </li>
                                            <li>
                                                <label>Equipo</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">devices_other</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input"
                                                        placeholder="Nombre del cliente" value="">
                                                </div>
                                            </li>
                                            <li>
                                                <label>Fecha limite</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">event</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input datetimepicker"
                                                        placeholder="Fecha limite" value="">
                                                </div>
                                            </li>
                                            <li>
                                                <label>Fecha de ingreso</label>
                                                <div class="input-group">
                                                    <div class="input-group-addon">
                                                        <span class="material-icons">today</span>
                                                    </div>
                                                    <input type="text" class="s-card-actv-input datetimepicker"
                                                        placeholder="Fecha limite" value="">
                                                </div>
                                            </li>
                                        </div>
                                    </div>
                                </ul>
                            </div>
                        </div>
                        <input class="catalog_search search-input s-card-actv-input" autocomplete="off" type="text" id="" name="catalog_search" placeholder="Buscar..">
                    </div>
                </div>
            </div>
        </div>
        <div class="nav-link col-xs-3 col-sm-4 col-md-4 col-lg-3 pull-right text-right">

            <div class="btn-group ">
                <button type="button" class="btn xl btn-more  btn-primary line dropdown-toggle " data-toggle="dropdown"
                    aria-expanded="true">
                    <span class="material-icons">add_circle_outline</span> <span
                        class="hidden-xs hidden-sm">Nuevo</span>
                </button>
                <ul class="dropdown-menu pull-right">
                    <li role="separator" class="divider"></li>
                    <li><a class="catalog_new_item" href="#catalog_new_item" data-toggle="modal" role="button"
                            title="Crear nuevo articulo"><i class="material-icons">store</i> Crear nueva orden</a>
                    </li>
                    <li role="separator" class="divider"></li>
                    <li><a href="#cajaRetiro" data-toggle="modal" role="button" title="Retirar efectivo"><i
                                class="material-icons">article</i> Listado de articulos </a></li>
                </ul>
            </div>

            <div class="btn-group ">
                <button type="button" class="btn btn-round xl dropdown-toggle" data-toggle="dropdown"
                    aria-expanded="true">
                    <span class="material-icons">settings</span>
                </button>
                 {{#with ws_lang_data}}
                <ul class="dropdown-menu pull-right">
                    <li>
                        <a href="#" onclick="new_board_star_intro()">
                            <i class="material-icons">dashboard</i>
                           {{m_new_board}}
                        </a>
                    </li>
                    <li>
                        <a href="#" onclick="catalog_config()">
                            <i class="material-icons">create</i>
                           {{m_edit_board}}  
                        </a>
                    </li>
                     <li>
                        <a href="#" onclick="catalog_config()">
                                <i class="material-icons"> view_carousel</i> 
                            {{m_new_board_group}}  
                        </a>
                    </li>
                    <li>
                        <a href="#" onclick="catalog_config()">
                                <i class="material-icons"> create</i> 
                            {{m_edit_board_group}}  
                        </a>
                    </li>
                    <li class="divider"></li>
                    <li>
                        <a href="#" id="new_board_group_btn" href="#new-stages" data-toggle="modal" role="button">
                            <i class="material-icons"> person_add</i> 
                             {{m_add_board_group_collaborator}}
                        </a>
                    </li>

{{/with}}
                </ul>
            </div>

        </div>
    </div>
</div>
</div>