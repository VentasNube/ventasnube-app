{{#with doc}}
<!-- CARD MAIN -->
<div class="board-item-content" onclick="get_edit_order(this)" oder_id="{{_id}}"
    board_type_name="{{#with ../../ws_lang_data}} {{{board_type_name}}} {{/with}}">
    <div class="s-card ">
        <!--s-card-body--->
        <div class="s-card-body br-primary ">
            <!-- s-card-header--->
            <div class="s-card-header padding-0 col-xs-12 col-sm-12 col-md-12 col-lg-12 ">
                <span class="left">
                    <div class="btn-group dopdown">
                        <!-- Con dopdown baja el poper-->
                        <span>
                            <span class="material-icons">
                                account_circle
                            </span>
                            {{customer.first_name}} {{customer.last_name}}
                        </span>
                    </div>
                </span>
                <!--s-detail--->
                <div class="right">
                    <div class="btn-group dopdown">
                        <!-- Prioridad -->
                        <button class="btn btn-round md pull-right" type="button" data-toggle="dropdown">
                            <div class="round-icon primary">
                                <span class="material-icons">sentiment_satisfied_alt</span>
                                {{priority.value}}
                            </div>
                        </button>
                    </div>
                    <div class="btn-group dopdown">
                        <button class="btn xs btn-more" type="button" data-toggle="dropdown">
                            <h5>
                                <span class="material-icons">update</span>
                                {{due_date.hour}}:{{due_date.minutes}}
                            </h5>
                        </button>
                    </div>
                </div>
                <!--s-card-content--->
            </div>
            <div class="padding-0 s-card-main col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <!--s-card-order--->
                <div class="text-center padding-10 btn s-card-order col-xs-4 col-sm-4 col-md-4 col-lg-4">
                    <div class="">
                        <span class="material-icons md">
                            qr_code
                        </span>
                    </div>
                    <span>{{order_id}} </span>
                </div>
                <!--s-card-content--->
                <div class="s-card-content col-xs-8 col-sm-8 col-md-8 col-lg-8">
                    {{#with equipment}}
                    <div or-id="1" class="equip-btn margin-b-5" data-toggle="tooltip" data-placement="bottom">
                        <span class="btn btn-default md">
                            {{type}} {{trade}}
                        </span>
                    </div>
                    {{/with}}
                    <div data-toggle="tooltip" data-placement="bottom">
                        <button or-id="1" class="presu-btn btn btn-primary md s-card-number" type="button">
                            $ {{total}}
                        </button>
                    </div>
                </div>
                <!-- s-card-main--->
            </div>
        </div>
        <!--s-card --->
    </div>
</div>
{{/with}}