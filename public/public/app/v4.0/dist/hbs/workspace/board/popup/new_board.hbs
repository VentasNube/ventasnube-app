            $('input.control_cash').iCheck('uncheck');
            $('input.control_cash').iCheck('disable');
        }
    });

    $(".btn-previus").click(function () {
        var step = $("#btn-previus").attr('step');
        if (step == 1) {
            $("#btn-next").text('Siguiente');
            $("#btn-previus").text('Cancelar');
            $(".btn-previus").attr('data-dismiss', 'modal');
            $(".btn-previus").attr('aria-label', 'Close');
        }
        else if (step == 2) {
            $("#btn-previus").attr('step', '1');
            $("#btn-previus").text('Volver');
            $("#step-1").show();
            $("#step-2").hide();
        }
        else if (step == 3) {
            $("#btn-next").text('Continuar');
            $("#btn-previus").attr('step', '2');
            $("#step-2").show();
            $("#step-3").hide();
        }
    });

  
