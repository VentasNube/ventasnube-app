    {{#each user_data}}
        <li class="margin-5">
            <a onclick="select_contact(this)" contact_id="{{_id}}" class="" >
                <div class="first-letter s-card-mini-img col-xs-2 col-sm-2 col-md-2 col-lg-2">
                    <div class="round-icon user m">
                        <span class="first-letter" data-first-letter="{{first_name}}"></span></div>
                </div>
                <div class="s-card-actv-item-prod col-xs-10 col-sm-10 col-md-10 col-lg-10">
                    <div class="s-card-actv-item-name">
                        {{first_name}} {{last_name}}
                    </div>
                    <div class="s-card-actv-item-name">
                        <span class="text-blue">{{phone}}</span>
                        <span class="">{{document_number}}</span>
                    </div>
                </div>
            </a>
        </li>
    {{/each}}