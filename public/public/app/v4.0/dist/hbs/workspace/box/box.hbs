<!-- MODULE BOX mov COPILED -->
<div class="content_box"></div>
    <div class="box-header clearfix">                 
       <ul class="pagination pagination-md no-margin pull-left">
            {{#each pages}}
                {{#if active}}
                    <li><a class="active"><b>{{pageNumber}}</b></a></li>
                {{else}}
                    <li><a href="#" onclick="get_page_box_items({{pageNumber}}, {{pageSize}})">{{pageNumber}}</a></li>
                {{/if}}
            {{/each}}
            <li><a href="#" onclick="get_page_box_items({{nextPage}}, {{pageSize}})">Próxima</a></li>
            <li><a href="#" onclick="get_page_box_items({{lastPage}}, {{pageSize}})">Última</a></li>
        </ul> 
        <div class="pagination-info pull-right">
            Mostrar
            <select onchange="change_page_size(this)">
                <option value="5">5</option>
                <option value="10">10</option>
                <option value="20">20</option>
                <option value="50">50</option>
            </select>
        </div>
    </div>
    <div id="content_box_commpiled" class="box-body no-padding">
    <!-- Imprimo Resultados -->
    </div>
    <div class="box-footer clearfix">                 
        <ul class="pagination pagination-md no-margin pull-left">
            {{#each pages}}
                {{#if active}}
                    <li><a class="active"><b>{{pageNumber}}</b></a></li>
                {{else}}
                    <li><a href="#" onclick="get_all_box_items({{pageNumber}}, {{pageSize}})">{{pageNumber}}</a></li>
                {{/if}}
            {{/each}}
            <li><a href="#" onclick="get_all_box_items({{nextPage}}, {{pageSize}})">Próxima</a></li>
            <li><a href="#" onclick="get_all_box_items({{lastPage}}, {{pageSize}})">Última</a></li>
        </ul> 
    </div>
</div>
