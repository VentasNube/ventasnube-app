 <!--- CATALOG BARRA NAVEGACION TOP TEMPLATE ( nav_bar.hbs )  --->
<div class="box">
    {{#with ws_lang_data }}
    <div class="box-header">
            <div class="nav-header nav_bar_catalog_header">
                <div class="padding-0 col-xs-9 col-sm-8 col-md-8 col-lg-8">
                    <div class="nav_bar_catalog_search" >
                            <div class="input-filters" >
                                    <div class="group">
                                                <div class="btn-group dopdown">
                                                    <button type="button" class="btn xl btn-more  dropdown-toggle btn-primary text" data-toggle="dropdown" aria-expanded="true">
                                                        {{b_filter}}
                                                        <span class="material-icons"> filter_list </span>
                                                    </button>
                                                    <ul class="dropdown-menu xl pull-left">
                                                        <!-- Con pull-right mueve para la derecha o hizquierda el poper-->
                                                        <div class="body">
                                                            <li class="header"> <span> {{#with ws_top_bar}} {{ws_lang_data.b_myaccount}}{{/with}}</span> Filtrar movimientos</li>
                                                            <li class="divider"></li>
                                                            <div class="padding-10 col-xs-12 col-sm-6 col-md-6">
                                                                <li>
                                                                    <label>Cliente  {{#each ws_lang_data.ws_lang_data}} {{b_myaccount}}{{/each}}</label>
                                                                    <div class="input-group">
                                                                        <div class="input-group-addon">
                                                                            <span class="material-icons">account_circle</span>
                                                                        </div>
                                                                        <input type="text" class="s-card-actv-input" placeholder="Nombre del cliente" value="">
                                                                    </div>
                                                                </li>
                                                                <li>
                                                                    <label>Colaborador</label>
                                                                    <div class="input-group">
                                                                        <div class="input-group-addon">
                                                                            <span class="material-icons">group</span>
                                                                        </div>
                                                                        <input type="text" class="s-card-actv-input" placeholder="Nombre del cliente"
                                                                            value="">
                                                                    </div>
                                                                </li>
                                                            </div>
                                                            <div class="padding-10  col-xs-12 col-sm-6 col-md-6">
                                                                <li>
                                                                    <label>Prioridad</label>
                                                                    <div class="input-group">
                                                                        <div class="input-group-addon">
                                                                            <span class="material-icons">low_priority</span>
                                                                        </div>
                                                                        <input type="text" class="s-card-actv-input" placeholder="Nombre del cliente"
                                                                            value="">
                                                                    </div>
                                                                </li>
                                                                <li>
                                                                    <label>Estado</label>
                                                                    <div class="input-group">
                                                                        <div class="input-group-addon">
                                                                            <span class="material-icons">mood</span>
                                                                        </div>
                                                                        <input type="text" class="s-card-actv-input" placeholder="Nombre del cliente"
                                                                            value="">
                                                                    </div>
                                                                </li>
                                                          
                                                                <li>
                                                                    <label>Fecha de ingreso</label>
                                                                    <div class="input-group">
                                                                        <div class="input-group-addon">
                                                                            <span class="material-icons">today</span>
                                                                        </div>
                                                                        <input type="text" class="s-card-actv-input datetimepicker"
                                                                            placeholder="Fecha limite" value="">
                                                                    </div>
                                                                </li>
                                                            </div>
                                                        </div>
                                                    </ul>
                                                </div>
                                    </div>
                                     <input class="contact_search s-card-actv-input" autocomplete="off" type="text" id="" name="contact_search" placeholder="{{input_contact_search}}">
                            </div>
                    </div>
                </div>
            </div>
            <div class="nav-link col-xs-3 col-sm-4 col-md-4 col-lg-3 pull-right text-right">
              
                    <div class="btn-group ">
                        <button type="button" class="btn xl btn-more  btn-primary line dropdown-toggle "
                            data-toggle="dropdown" aria-expanded="true">
                            <span class="material-icons">add_circle_outline</span> <span
                                class="hidden-xs hidden-sm">{{b_new}}</span>
                        </button>
                        <ul class="dropdown-menu pull-right">
                            <li role="separator" class="divider"></li>
                             <li><a  onclick="add_new_contact(this)" href="" data-toggle="modal" role="button"
                                    title="{{address_title}}"><i class="material-icons">add</i>Gasto</a>
                            </li>
                            <li><a  onclick="add_new_contact(this)" href="" data-toggle="modal" role="button"
                                    title="{{address_title}}"><i class="material-icons">add</i>Ingreso</a>
                            </li>
                            <li><a  onclick="add_new_contact(this)" href="" data-toggle="modal" role="button"
                                    title="{{address_title}}"><i class="material-icons">add</i>Retiro efectivo</a>
                            </li>
                             <li><a  onclick="add_new_contact(this)" href="" data-toggle="modal" role="button"
                                    title="{{address_title}}"><i class="material-icons">add</i>Ingreso de efectivo</a>
                            </li>
                            <li role="separator" class="divider"></li>
                               <li><a  onclick="add_new_contact(this)" href="" data-toggle="modal" role="button"
                                    title="{{address_title}}"><i class="material-icons">add</i>Cierre de CAJA</a>
                            </li>
                        </ul>
                    </div>
             
                <div class="btn-group ">
                    <button type="button" class="btn btn-round xl dropdown-toggle" data-toggle="dropdown"
                        aria-expanded="true">
                        <span class="material-icons">settings</span>
                    </button>
                    <ul class="dropdown-menu pull-right">
                        <!--li>
                            <a onclick="catalog_config()">
                                <i class="material-icons">create</i>
                                Editar catalogo
                            </a>    
                              </li-->
                        <li class="divider"></li>
                        <li><a id="new_board_group_btn" href="#new-stages" data-toggle="modal" role="button"><i
                                    class="material-icons">view_carousel</i>{{m_add_board_group_collaborator}}</a></li>
                    </ul>
                </div>
                
            </div>
        </div>
    {{/with}}
    </div>
</div>
