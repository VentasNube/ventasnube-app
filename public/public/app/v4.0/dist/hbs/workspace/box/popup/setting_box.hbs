        var tabToActivate = $('.nav-tabs li[for="' + radioId + '"]');
        console.log('tabToActivate:', tabToActivate);

        if (tabToActivate.length > 0) {
            // Activa el tab correspondiente
            tabToActivate.find('a').tab('show');
            // Cambia la clase activa de los tabs
            $('.nav-tabs li').removeClass('active');
            tabToActivate.addClass('active');
        }
    });

    // Maneja el clic en los tabs para activar el radio button correspondiente
    $('.nav-tabs li').on('click', function () {
        var radioId = $(this).attr('for');

