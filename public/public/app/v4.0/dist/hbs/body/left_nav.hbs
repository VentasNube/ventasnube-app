<section class="sidebar">
    <ul class="sidebar-menu">
        {{#each ws_left_nav.m}}
            <li class="treeview">
                 <a
                    {{#ifEquals m_type_action "1"}} 
                        class="l_nav_m"  m_id="{{{m_id}}}"  m_url="{{m_url}}" href="{{m_url}}"
                        {{else}}
                        m_id="{{{m_id}}}" s_url_t_m="{{{m_url}}}" href="#"                   
                    {{/ifEquals}}
                    >
                    <i class="material-icons">{{m_icon}}</i>
                    <span>{{m_name}} </span>
                </a>
                <ul class="treeview-menu">
                    {{#each ../ws_left_nav.m_t}}
                         {{#ifEquals m_id ../m_id }}   
                            <li class="active">
                                <a class="l_nav_t_m"
                                    {{#ifEquals m_t_type_action "1"}}   
                                             href="{{m_t_url}}"
                                        {{else}}
                                            m_id="{{{m_id}}}" m_t_id="{{{m_t_id}}}" s_url_t_m="{{{../m_url}}}"  href="#"                                       
                                    {{/ifEquals}}
                                 >
                                    <span class="material-icons">{{m_t_icon}}</span>
                                    <span class="this_m_t_name">{{m_t_name}}</span>                                
                                </a>
                            </li>
                        {{/ifEquals}}

                    {{/each}}
                </ul>
            </li>
        {{/each}}
        <li class="treeview  active">
                <a class="l_nav_m"  m_id=""  m_url="" href=""   >
                    <i class="material-icons">manage_accounts</i>
                    <span>Mi cuenta</span>
                </a>
                <ul class="treeview-menu">
                            <li class="active">
                                <a class="l_nav_t_m"  m_id="" m_t_id="" s_url_t_m=""  href="#"    >
                                    <span class="material-icons">shopping_cart</span>
                                    <span class="this_m_t_name">Mi carrito </span>
                                </a>
                            </li>
                            <li class="active">
                                <a class="l_nav_t_m"  m_id="" m_t_id="" s_url_t_m=""  href="#"    >
                                    <span class="material-icons">favorite</span>
                                    <span class="this_m_t_name">Favoritos </span>                                
                                </a>
                            </li>
                            <li class="active">
                                <a class="l_nav_t_m"  m_id="" m_t_id="" s_url_t_m=""  href="#"    >
                                    <span class="material-icons">local_mall</span>
                                    <span class="this_m_t_name">Mis compras  </span>                                
                                </a>
                            </li>   
                            <li class="active">
                                <a class="l_nav_t_m"  m_id="" m_t_id="" s_url_t_m=""  href="#"    >
                                    <span class="material-icons">app_settings_alt</span>
                                    <span class="this_m_t_name">Mis datos</span>
                                </a>
                            </li>                       
                            <li class="active">
                                <a class="l_nav_t_m"  m_id="" m_t_id="" s_url_t_m=""  href="#"    >
                                    <span class="material-icons">group_work</span>
                                    <span class="this_m_t_name">Espacios de trabajo</span>
                                </a>
                            </li>
                </ul>
        </li>
    </ul>
</section>      